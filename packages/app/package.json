{
  "name": "@token-factory/app",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "run-p  lint:prettier lint:eslint",
    "lint:prettier": "prettier --plugin-search-dir . --check '**/*.{js,ts,svelte,md,json,html,css,cjs}' .",
    "lint:eslint": "eslint . --max-warnings=0",
    "format": "run-p format:eslint format:prettier",
    "format:prettier": "prettier --plugin-search-dir . --write '**/*.{js,ts,svelte,md,json,html,css,cjs}' .",
    "format:eslint": "eslint . --fix --max-warnings=0"
  },
  "devDependencies": {
    "@playwright/test": "1.25.0",
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.40",
    "@sveltejs/kit": "next",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@types/plotly.js": "^2.12.13",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.41.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.19",
    "postcss-import": "^15.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "prism-themes": "^1.9.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-prism": "^1.1.6",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.3.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "plotly.js-dist-min": "^2.18.0",
    "uplot": "^1.6.23"
  }
}
